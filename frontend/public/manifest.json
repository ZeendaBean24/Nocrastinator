{
  "manifest_version": 2,
  "short_name": "Focus!",
  "name": "Nocrastinator!",
  "version": "1.0",
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Procrastination Blocker"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  }
}